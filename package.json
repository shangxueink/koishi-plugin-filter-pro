  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hoshino-Yumetsuki/koishi-plugin-filter-pro",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/Hoshino-Yumetsuki/koishi-plugin-filter-pro/issues"
