  "koishi": {
    "browser": true,
    "category": "tool"
  },
  "peerDependencies": {
    "@koishijs/plugin-console": "^5.11.0",
    "koishi": "^4.18.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@koishijs/client": "^5.30.11",
    "@koishijs/plugin-console": "^5.30.11",
    "@koishijs/plugin-proxy-agent": "^0.3.3",
    "@types/node": "^25.3.0",
